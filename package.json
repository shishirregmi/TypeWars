{
  "name": "typewars",
  "displayName": "TypeWars",
  "description": "This is a game by developer and a game for  developer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typewars.startListening",
        "title": "TypeWars: Start Listening"
      },
      {
        "command": "typewars.stopListening",
        "title": "TypeWars: Stop Listening"
      },
      {
        "command": "typewars.startGame",
        "title": "TypeWars: Start Game"
      },
      {
        "command": "typewars.stopGame",
        "title": "TypeWars: Stop Game"
      }
    ],
    "configuration": {
      "properties": {
          "TypeWars.listenCount": {
              "type": "integer",
              "default": 0
          },
          "TypeWars.playerXp": {
              "type": "integer",
              "default": 0
          },
          "TypeWars.listenKeys": {
              "type": "boolean",
              "default": true
          }
      }
  }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
